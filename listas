lista1 = []
lista2 = []
lista3 = []
t1 = int(input('Tamanho da Lista 1: '))
t2 = int(input('Tamanho da Lista 2: '))
for c1 in range (1, t1+1):
  valores = int(input('Digite o valor 1: '))
  lista1.append(valores)
  lista3.append(valores)
print()
for c2 in range (1, t2+1):
  valores1 = int(input('Digite o valor 2: '))
  lista2.append(valores1)
  lista3.append(valores1)
lista3.sort()
print(lista3)
lista3.sort(reverse=True)
print(lista3)
